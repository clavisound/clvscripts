#!/bin/bash
# wait for usbmuxd
sleep 3

FILE=/var/log/iOS.GasGauge

sleep 3
MESSAGE=$(ideviceinfo --simple | grep DeviceName)
MESSAGE+=","
MESSAGE+=`date -R`
MESSAGE+=","
MESSAGE+=$(idevicediagnostics diagnostics GasGauge | grep integer | sed -e 's+.*<integer>+,+' -e 's+</int.*+,+')

echo -e $MESSAGE >> $FILE
